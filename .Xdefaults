!Xft.dpi:                96
Xft.dpi:                192
Xft.antialias:          true
Xft.rgba:               none
Xft.hinting:            true
Xft.autohint:           true
Xft.hintstyle:          hintsligh

URxvt.termName:         rxvt-unicode-256color
URxvt.depth:            32
!URxvt.letterSpace:      -5
URxvt.scrollBar:        false
URxvt.font:             xft:DejaVu Sans Mono for Powerline:size=12:antialias=true
URxvt*boldFont:         xft:DejaVu Sans Mono for Powerline:bold:size=12

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      xdg-open
URxvt.matcher.button:   1

! special
*.foreground:   #b7b8b9
*.background:   #0c0d0e
*.cursorColor:  #b7b8b9

! black
*.color0:       #0c0d0e
*.color8:       #737475

! red
*.color1:       #e31a1c
*.color9:       #e31a1c

! green
*.color2:       #31a354
*.color10:      #31a354

! yellow
*.color3:       #dca060
*.color11:      #dca060

! blue
*.color4:       #3182bd
*.color12:      #3182bd

! magenta
*.color5:       #756bb1
*.color13:      #756bb1

! cyan
*.color6:       #80b1d3
*.color14:      #80b1d3

! white
*.color7:       #b7b8b9
*.color15:      #fcfdfe
